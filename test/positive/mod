
option iptables
option automod

chain INPUT {
limit 1/s ACCEPT;
mac 00:00:00:00:00:00 ACCEPT;
uid 0 ACCEPT;
state INVALID ACCEPT;
tos 0x02 ACCEPT;
mark 3 ACCEPT;
}

chain OUTPUT {
mod limit limit 1/s ACCEPT;
mod mac mac 00:00:00:00:00:00 ACCEPT;
mod owner uid 0 ACCEPT;
mod state state INVALID ACCEPT;
mod tos tos 0x02 ACCEPT;
mod mark mark 3 ACCEPT;
}


